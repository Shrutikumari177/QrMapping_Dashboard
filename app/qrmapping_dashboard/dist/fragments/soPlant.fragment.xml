<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog
        id="plantValueHelpValueHelp"
        noDataText="Loading..."
        title="Select Plant"
        search="onplantValueHelpSearch"
        liveChange="onplantValueHelpSearch"
        confirm="onplantValueConfirmItem"
       items="{plantDataModel>/}"
        initialFocus="SearchField">
        <StandardListItem id="_IDGenStandardListItem7" 
            title="{plantDataModel>Plant}"
            description="{plantDataModel>PlantDesc}"
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>
