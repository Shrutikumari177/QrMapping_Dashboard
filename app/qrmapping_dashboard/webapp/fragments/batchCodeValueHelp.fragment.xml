<core:FragmentDefinition
    xmlns="sap.m"
    xmlns:core="sap.ui.core">
    <SelectDialog 
        id="batchValueHelpValueHelp1"
        noDataText="Loading..."
        title="Select Batch ID"
        search="onBatchValueHelpSearch"
        liveChange="onBatchValueHelpSearch"
        confirm="onBatchValueConfirmItem"
        items="{/zbatchno_track}"
        initialFocus="SearchField"
        
        
        >
        <StandardListItem 
            id="batchValueHelpValueHelpStandardListItem"
            title="{BatchNo}"
            description=""
            iconDensityAware="false"
            iconInset="false"
            type="Active" />
    </SelectDialog>
</core:FragmentDefinition>
